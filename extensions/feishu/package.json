{
  "name": "@openclaw/feishu",
  "version": "2026.1.29",
  "type": "module",
  "description": "OpenClaw Feishu (Lark) channel plugin",
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "feishu",
